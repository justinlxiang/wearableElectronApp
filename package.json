{
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2"
  },
  "name": "wearableui",
  "version": "1.0.0",
  "main": "main.js",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "scripts": {
    "start": "concurrently --prefix \"[{name}]\" --names \"PYTHON,PROXY,ELECTRON\" \"python app.py\" \"node proxyServer.js\" \"npm run electron\"",
    "electron": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.wearableui",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlxiang/wearableRobotUI.git"
  },

{
  "type": "commonjs",
  "dependencies": {
    "asar": "^3.2.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2"
  },
  "name": "wearableui",
  "version": "1.0.0",
  "main": "main.js",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "scripts": {
    "start": "npm run electron",
    "start-dev": "npm run electron",
    "electron": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.wearableui",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "main.js",
      "app.py",
      "db.py",
      "index.html",
      "proxyServer.js",
      "public/**/*",
      "node_modules/**/*",
      "package.json",
      "images/**/*",
      "instance/**/*",
      "scripts/**/*",
      "static/**/*",
      "styles/**/*"
    ],
    "extraResources": [
      {
        "from": "app.py",
        "to": "./app.py"
      },
      {
        "from": "proxyServer.js",
        "to": "./proxyServer.js"
      },
      {
        "from": "db.py",
        "to": "./db.py"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlxiang/wearableRobotUI.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justinlxiang/wearableRobotUI/issues"
  },
  "homepage": "https://github.com/justinlxiang/wearableRobotUI#readme",
  "description": ""
}

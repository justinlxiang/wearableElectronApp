{
  "type": "commonjs",
  "dependencies": {
    "asar": "^3.2.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "http-proxy-middleware": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2"
  },
  "name": "wearableui",
  "productName": "Wearable Gesture Control",
  "version": "1.0.0",
  "main": "main.js",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "scripts": {
    "start": "npm run electron",
    "electron": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postbuild": "chmod +x dist/mac-arm64/wearableui.app/Contents/Resources/bin/pyApp && chmod +x dist/mac-arm64/wearableui.app/Contents/Resources/bin/proxyServer"
  },
  "build": {
    "appId": "com.example.wearableui",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraResources": [
      {
        "from": "bin/",
        "to": "bin/",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlxiang/wearableRobotUI.git"
  },
  "keywords": [],
  "author": "Justin Xiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justinlxiang/wearableRobotUI/issues"
  },
  "homepage": "https://github.com/justinlxiang/wearableRobotUI#readme",
  "description": "Interface for MindRove Gesture customization and model training"
}
